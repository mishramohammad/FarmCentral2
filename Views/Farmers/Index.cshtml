@model IEnumerable<FarmCentral2.Models.Farmers>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Fname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fsurname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Femail)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fusername)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fpassword)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fsurname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Femail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fusername)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fpassword)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
